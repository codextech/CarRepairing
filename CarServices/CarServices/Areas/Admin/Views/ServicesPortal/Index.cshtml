@model IEnumerable<CarServices.Models.Service>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}





<div class="page-wrapper">
   
    <div class="container-fluid">


        <div class="row">


            @foreach (var item in Model)
            {
            <!-- .col -->
                <div class="col-md-6 col-lg-6 col-xlg-4">
                    <div class="card card-body">
                        <div class="row align-items-center">
                            <div class="col-md-4 col-lg-3 text-center">
                                <a href="app-contact-detail.html"><img src="@Url.Content(item.ImageUrl)" alt="user" class="img-circle img-fluid"></a>
                            </div>
                            <div class="col-md-8 col-lg-9">
                                <h3 class="box-title m-b-0">@item.ServiceName</h3>
                                <p>
                                    @item.Description
                                    <br />
                                    <br />
                                    <span>@item.Price</span>
                                    <span style="margin-left:150px">
                                        @Html.ActionLink("Edit", "Edit", new {id= item.ServiceID },  new { @class = "btn btn-dark" })
                                        @Html.ActionLink("Delete", "Delete", new { id = item.ServiceID }, new { @class = "btn btn-dark" })

                                    </span>
                                </p>
                            </div>
                        </div>
                    </div>



                </div>
            }
            <input type="button" value="Test" id="tbtn" />

        </div>
    </div>
</div>

<script>

    $('#tbtn').click(function () {
      
        localStorage.setItem("Nameof Emplyee", "Val");
    });
</script>


